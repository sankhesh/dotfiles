[
  {
    "clean": [
      "~",
      "~/.config/terminator",
      "~/.vim",
      "~/.vim/bundle",
      "~/.vim/ftplugin",
      "~/.vim/UltiSnips"
    ]
  },

  {
    "link":
      {
        "~/.bash_profile": "bash_profile",
        "~/.bash_prompt": "bash_prompt",
        "~/.config/terminator/config":
          create: true
          path: "terminator_config"
        "~/.gitconfig": "gitconfig",
        "~/.vimrc": "vimrc",
        "~/.vim": "vim/",
      }
  },

  {
  "shell":
    [
      ["git submodule update --init --recursive", "Installing submodules"]
    ]
  }
]
